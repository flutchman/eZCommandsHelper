{
    "name": "flutchman/ez-commands-helper",
    "license": "GPL-2.0",
    "type": "ezplatform-bundle",
    "description": "eZ Platform Commands Helper",
    "keywords": [
        "development",
        "bash",
        "ezpublish",
        "ezplatform"
    ],
    "authors": [
        {
            "name": "Alexandre Andrieux",
            "email": "alexandre.andrieux.pro@gmail.com"
        }
    ],
    "support": {
        "email": "alexandre.andrieux.pro@gmail.com",
        "source": "https://github.com/flutchman/eZCommandsHelper",
        "docs": "https://github.com/flutchman/eZCommandsHelper/tree/master/docs",
        "issues": "https://github.com/flutchman/eZCommandsHelper/issues"
    },
    "autoload": {
        "psr-4": {
            "Flutchman\\eZCommandsHelper\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 3000,
        "platform": {
            "php": "7.0"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "symfony/var-dumper": "~3.4",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^2.9",
        "phpunit/phpunit": "^5.7",
        "behat/behat": "^3.4",
        "mikey179/vfsStream": "^1.6",
        "twig/twig": "^1.35",
        "symfony/translation": "~3.4",
        "symfony/twig-bridge": "~3.4"
    },
    "require": {
        "php": ">=7.0",
        "symfony/console": "~3.4",
        "symfony/yaml": "~3.4",
        "symfony/dependency-injection": "~3.4",
        "symfony/process": "~3.4",
        "symfony/finder": "~3.4",
        "symfony/config": "~3.4",
        "symfony/filesystem": "~3.4",
        "novactive/collection": "^1.0",
        "symfony/options-resolver": "~3.4",
        "padraic/phar-updater": "^1.0.6",
        "symfony/event-dispatcher": "~3.4",
        "nesbot/carbon": "^1.22",
        "ezsystems/ezplatform": "^2.5"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "scripts-descriptions": {
        "fix-cs": "Fix Coding standard issues in current checkout."
    },
    "extra": {
        "_ezplatform_branch_for_behat_tests": "master",
        "branch-alias": {
            "dev-tmp_ci_branch": "master-dev"
        },
        "thanks": {
            "name": "flutchman/ez-commands-helper",
            "url": "https://github.com/flutchman/eZCommandsHelper"
        }
    },
    "bin": [
        "bin/ez-cmd"
    ]
}
